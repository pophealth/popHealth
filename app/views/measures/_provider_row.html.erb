<div class="provider row-fluid" data-provider="<%=provider_row.id%>">
  <div class="span6"><%= link_to formatted_provider_name(provider_row), measures_path(npi: provider_row.npi) %>
  </div>
  <div class="percent span3">
    <div class="measureProviderPopulationPercentage"></div>
  </div>
  <div class="fraction span3">
    <%= render :partial => "shared/fraction", locals: {numerator: 0, denominator: 0} %>
  </div>
</div>