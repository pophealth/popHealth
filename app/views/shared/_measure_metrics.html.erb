    <h1><%= display_raw_percentage(numerator, denominator) %></h1>
  </div> 
  
  <div style="text-align:right" class='span5' >
    <%= render :partial => "shared/fraction", locals: {numerator: numerator, denominator: denominator}%>
  </div> 
</div>
