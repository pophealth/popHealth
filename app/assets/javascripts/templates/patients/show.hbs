<div class="row">
  <div class="left-sidebar">
    <h1><i class="fa fa-tasks"></i> measures</h1>
    <div class="panel-group">
      <h2>Outliers</h2>
      <div class="panel-heading active">
        <div class="indicator">
          <i class="fa fa-exclamation-triangle"></i>
        </div>
        <div class="measure-title">Preventive Care And Screening: Tobacco - Use Assessment
        </div>
      </div>
    </div>
    <div class="panel-heading">
      <div class="indicator">
        <i class="fa fa-exclamation-triangle"></i>
      </div>
      <div class="measure-title">Smoking And Tobacco Use Cessation, Medical Assistance - Advising Smokers And Tobacco Users To Quit
      </div>
    </div>
  </div>

  <div class="main">
    <h1 class="main-head"><i class="fa fa-user"></i> {{first}} {{last}}</h1>
    <div id="time-bar"></div>
    <div class="demo col-md-offset-1">
      <dl class="dl-demo dl-horizontal">
        <dt>RECORD NUMBER</dt>
        <dd>730132778</dd>
        <dt>EFFECTIVE DATE</dt>
        <dd>{{effective_time}}</dd>
        <dt>DOB</dt>
        <dd>{{birthdate}}</dd>
        <dt>GENDER</dt>
        <dd>{{gender}}</dd>
      </dl>
      <dl class="dl-demo dl-horizontal">
        <dt>RACE</dt>
        <dd>{{race}}</dd>
        <dt>ETHNICITY</dt>
        <dd>{{ethnicity}}</dd>
        <dt>LANGUAGES</dt>
        <dd>{{languages}}</dd>
        <dt>PROVIDERS</dt>
        <dd>Jonathan Cooper MD</dd>
      </dl>
    </div>
    <div class="time-line">
      <div class="timeline-title">Patient History Relative to Measures</div>
    </div>
    {{#collection entries item-view="EntryView"}}
      <div class="record-listing">
        <div class="circle-icon">
          <i class="fa fa-{{icon}}"></i>
        </div>
        <div class="patient-data">
          <dl class="dl-record dl-horizontal">
            <dt>{{entry_type}}</dt>
            {{#if description}}
              <dd>{{description}}</dd>
            {{else}}
              <dd class="status-unavailable">No Details Available</dd>
            {{/if}}
          </dl>
          <dl class="dl-date dl-horizontal">
            <dt>date</dt>
            <dd>{{start_time}} {{#if end_time}} &ndash; {{end_time}} {{/if}}</dd>
          </dl>
        </div>
      </div>
    {{/collection}}
    </div>
  </div>
</div>